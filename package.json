{
  "name": "generate-pr-mcp",
  "version": "1.1.1",
  "description": "Zero-configuration MCP server for Claude Desktop and Cursor IDE that generates intelligent PRs using built-in AI - no API keys required",
  "type": "module",
  "bin": {
    "mcp-server": "./build/index.js"
  },
  "files": [
    "build"
  ],
  "scripts": {
    "build": "tsc && node -e \"require('fs').chmodSync('build/index.js', '755')\"",
    "prepare": "npm run build",
    "watch": "tsc --watch",
    "inspector": "npx @modelcontextprotocol/inspector build/index.js"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^0.6.0",
    "vscode-uri": "^3.1.0",
    "zod": "^3.25.63"
  },
  "devDependencies": {
    "@types/node": "^20.11.24",
    "typescript": "^5.3.3"
  },
  "keywords": [
    "mcp",
    "pr",
    "pull-request",
    "generator",
    "cursor",
    "cursor-ide",
    "claude-desktop",
    "zero-config",
    "no-api-keys",
    "ai",
    "code-analysis",
    "git-diff",
    "automation"
  ],
  "author": "Your Name",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/kpndevroot/generate-pr-mcp.git"
  }
}
